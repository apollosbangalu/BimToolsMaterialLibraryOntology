###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#correlatesWith
:correlatesWith rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#MaterialProperty> ;
                rdfs:range <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#MaterialProperty> ;
                rdfs:comment "Indicates correlation between material properties"@en ;
                rdfs:label "correlates with"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasCategory
:hasCategory rdf:type owl:ObjectProperty ;
             owl:inverseOf :isCategoryOf ;
             rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ;
             rdfs:range :MaterialCategory ;
             <http://purl.org/dc/elements/1.1/description> "Links a building material instance to its corresponding material category for taxonomic classification within the BIM materials hierarchy"@en ;
             rdfs:comment "Associates a building material with its material category classification"@en ;
             rdfs:label "has Category"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasExternalReferenceDatabase
:hasExternalReferenceDatabase rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              owl:inverseOf :isExternalReferenceDatabaseOf ;
                              rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ;
                              rdfs:range :ExternalReferenceDatabase ;
                              <http://purl.org/dc/elements/1.1/description> "Establishes connection between building material and external databases such as classification systems, standards, or LCA databases for extended material data"@en ;
                              rdfs:comment "Links a building material to an external reference database for additional material information"@en ;
                              rdfs:label "has External Reference Database"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasMaterial
:hasMaterial rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :isMaterialOf ;
             rdfs:domain :BuildingElement ;
             rdfs:range <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ;
             <http://purl.org/dc/elements/1.1/description> "Defines the material composition relationship between building elements and their constituent materials in BIM models"@en ;
             rdfs:comment "Associates a building element with its constituent building material"@en ;
             rdfs:label "has Material"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasMaterialDefinition
:hasMaterialDefinition rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isMaterialDefinitionOf ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ;
                       rdfs:range <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#MaterialDefinition> ;
                       <http://purl.org/dc/elements/1.1/description> "Connects building materials to their comprehensive definitions including composition, layers, profiles, or constituent specifications"@en ;
                       rdfs:comment "Links a building material to its detailed material definition specification"@en ;
                       rdfs:label "has Material Definition"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasMaterialProperty
:hasMaterialProperty rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isPropertyOf ;
                     rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ;
                     rdfs:range <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#MaterialProperty> ;
                     <http://purl.org/dc/elements/1.1/description> "Establishes the relationship between building materials and their measurable characteristics such as mechanical, thermal, physical, or behavioral properties"@en ;
                     rdfs:comment "Associates a building material with its specific material properties"@en ;
                     rdfs:label "has Material Property"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasPrimaryCategory
:hasPrimaryCategory rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isPrimaryCategoryOf ;
                    rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ;
                    rdfs:range :MaterialCategory ;
                    <http://purl.org/dc/elements/1.1/description> "Establishes the primary taxonomic classification relationship between building material instances and their main category types such as Wood, Concrete, Metal, or Plastic within the BIM materials hierarchy"@en ;
                    rdfs:comment "Associates a building material with its primary category classification at the highest level of the material taxonomy"@en ;
                    rdfs:label "has Primary Category"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasSecondaryCategory
:hasSecondaryCategory rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :hasCategory ;
                      owl:inverseOf :isSecondaryCategoryOf ;
                      rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ;
                      rdfs:range :MaterialCategory ;
                      <http://purl.org/dc/elements/1.1/description> "Establishes the secondary taxonomic classification relationship between building material instances and their specific subcategory types such as HardWood, Standard, Steel, or Thermoplastic within the detailed BIM materials hierarchy"@en ;
                      rdfs:comment "Associates a building material with its secondary category classification providing more specific categorization within the primary material type"@en ;
                      rdfs:label "has Secondary Category"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#hasUnit
:hasUnit rdf:type owl:ObjectProperty ;
         owl:inverseOf :isUnitOf ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#MaterialProperty> ;
