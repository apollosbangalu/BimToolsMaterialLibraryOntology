###  http://qudt.org/schema/qudt/Unit
<http://qudt.org/schema/qudt/Unit> rdf:type owl:Class ;
                                   owl:disjointWith :BuildingElement ,
                                                    :ExternalReferenceDatabase ,
                                                    :MaterialCategory ,
                                                    <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#BuildingMaterial> ,
                                                    <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#MaterialDefinition> ,
                                                    <https://w3id.org/digitalconstruction/0.3/BuildingMaterials#MaterialProperty> ;
                                   :examples " Square meters for area, Kilograms for weight             " ;
                                   rdfs:comment " Represents the unit of measurement for various material properties, ensuring consistency in calculations." ;
                                   rdfs:label "Unit"@en .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#AcousticCeilingTile
:AcousticCeilingTile rdf:type owl:Class ;
                     rdfs:subClassOf :Ceiling ;
                     :examples " Acoustic tiles for theaters, Sound-absorbing ceiling panels     " ;
                     rdfs:comment " Represents acoustic ceiling tile materials designed to absorb sound and improve room acoustics.   " ;
                     rdfs:label "Acoustic Ceiling Tile" .


###  http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology#Aluminum
:Aluminum rdf:type owl:Class ;
          rdfs:subClassOf :Metal ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :Density ;
                            owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:double ;
                                                owl:withRestrictions ( [ xsd:minInclusive "2680.0"^^xsd:double
                                                                       ]
                                                                       [ xsd:maxInclusive "2810.0"^^xsd:double
                                                                       ]
                                                                     )
                                              ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :TensileStrength ;
                            owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:double ;
                                                owl:withRestrictions ( [ xsd:minInclusive "89.6"^^xsd:double
                                                                       ]
                                                                       [ xsd:maxInclusive "455.1"^^xsd:double
                                                                       ]
                                                                     )
                                              ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :YieldStrength ;
                            owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                owl:onDatatype xsd:double ;
                                                owl:withRestrictions ( [ xsd:minInclusive "34.5"^^xsd:double
                                                                       ]
                                                                       [ xsd:maxInclusive "399.9"^^xsd:double
                                                                       ]
                                                                     )
                                              ]
                          ] ;
          :examples "Aluminum sheets for roofing, Extruded aluminum profiles for windows" ;
          rdfs:comment "Represents aluminum materials, known for their lightweight and corrosion-resistant properties used in various construction applications."@en ;
          rdfs:label "Aluminum"@en .


