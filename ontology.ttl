                                                                       :preferredNamespacePrefix "bimtool"@en ;
                                                                       :preferredNamespaceUri "http://www.BimToolsMaterialLibrary.com/BimBuildingMaterialsOntology"@en ;
                                                                       rdfs:label "Building Information Modelling (BIM) Tools Material Library Ontology"@en ;
                                                                       owl:versionInfo 0.1 .

